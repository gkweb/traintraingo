{
  "version": 2,
  "builds": [
    { "src": "client/package.json", "use": "@now/next" },
    { "src": "client/next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/client/$1" },
    { "src": "/departures/(?<stop_id>[^/]+)$", "dest": "/client/departures?stop_id=$stop_id" }
  ]
}
